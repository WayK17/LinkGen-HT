{
  "version": 2,
  "builds": [
    {
      "src": "mediafire-extract-api.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api(.*)",
      "dest": "/mediafire-extract-api.js"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}